                                                text, "Euphoria";
                                                font, "Vera";   /* Only font Vera works!  Use other and segv */
                                                size, 8;       /* Font Size */
                                                align,   0.5 1.0;       /* Text alignment based on rel1/rel2 */
                                        }
                                }
                        }

