                        /* LCD Backdrop Image */
                        part {
                                name, "lcd_backdrop";
                                type, IMAGE;
                                mouse_events, 0;
                                description {
                                        state,  "default" 0.0;

                                        /* Part should fill from the far left, 20 pixels down */
                                        rel1 {
                                                relative, 0.0 0.0;
                                                offset, 5 16;
                                        }

                                        /* all the way to the far right, 40 pixels north of the bottom right corner */
