                                                relative, 1.0 0.0;
                                                offset, -6 81;
                                        }

                                        image {
                                                normal, "lcd_area.png";
                                                border, 5 5 5 5;
                                        }

                                        fill {
                                                smooth, 0;
                                        }
                                }
