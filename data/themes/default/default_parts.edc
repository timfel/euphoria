                        /* LCD Clipping Area - Invisible Rect */
                        part {
                                name, "lcd_clip";
                                type, RECT;
                                mouse_events, 0;
                                description {
                                        state,  "default" 0.0;

                                        rel1 {
                                                relative, 0.0 0.0;
